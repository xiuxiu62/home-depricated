*.loginshell        : true
*.savelines         : 25000

*.scrollBar         : false 
*.selectToClipboard : true

*faceName : Iosevka
*faceSize : 12

Xcursor.theme : Neutral
Xcursor.size  : 16

*.background  : #111111
*.foreground  : #dddddd

! black
*.color0  : #263640
*.color8  : #4a697d

! red
*.color1  : #d12f2c
*.color9  : #fa3935

! green
*.color2  : #819400
*.color10 : #a4bd00

! yellow
*.color3  : #b08500
*.color11 : #d9a400

! blue
*.color4  : #2587cc
*.color12 : #2ca2f5

! magenta
*.color5  : #696ebf
*.color13 : #8086e8

! cyan
*.color6  : #289c93
*.color14 : #33c5ba

! white
*.color7  : #bfbaac
*.color15 : #fdf6e3

URxvt.colorDB : #ffffff
URxvt.colorUL : #4682b4

URxvt.perl-ext-common : default,matcher,clipboard,resize-font,url-select,keyboard-select
URxvt.font : xft:Iosevka:regular:size=12:antialias=true:hinting=true

URxvt.internalBorder  : 5
URxvt.iso14755        : true
URxvt*depth           : 32

URxvt.keysym.M-x   : perl:keyboard-select:activate
URxvt.keysym.M-c   : perl:clipboard:copy
URxvt.keysym.M-v   : perl:clipboard:paste
URxvt.keysym.M-C-v : perl:clipboard:paste_escaped
URxvt.keysym.M-s   : perl:keyboard-select:search
URxvt.keysym.M-u   : perl:url-select:select_next

URxvt.url-select.launcher  : brave
URxvt.url-select.autocopy  : true
URxvt.url-select.underline : true 
URxvt.url-select.button    : 1

URxvt.resize-font.smaller : M-j
URxvt.resize-font.bigger  : M-k

xterm*faceName : Iosevka
xterm*faceSize : 12

Xft.lcdfilter : lcddefault
Xft.hintstyle : hintslight
Xft.hinting   : true
Xft.antialias : true
Xft.rgba      : rgb

